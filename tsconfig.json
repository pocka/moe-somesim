{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": true,
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["DOM", "ES2020"],
    "noEmit": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": ["vite/client"]
  },
  "include": ["src/**/*.ts"]
}
